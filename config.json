          "fallbackUrl": "https://play.google.com/store/search?q={args}"
        },
        {
          "supportedDevices": ["iOS"],
          "defaultUrl": "https://apps.apple.com",
          "customPatterns": [
            {
              "pattern": "developer/{dev_name}",
              "targetUrl": "https://apps.apple.com/developer/{dev_name}"
